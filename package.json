{
  "name": "llm-arena",
  "version": "1.0.0",
  "description": "A competitive, turn-based AI evaluation platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && npm run build:css",
    "build:css": "npx tailwindcss -i ./src/styles/input.css -o ./public/css/style.css",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "dev": "concurrently \"npx tsc -w\" \"npm run build:css -- --watch\" \"nodemon dist/server.js\" \"nodemon dist/worker.js\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "llm",
    "arena",
    "ai",
    "competition",
    "nodejs",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.0.0",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "pino": "^9.0.0",
    "pino-http": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/csurf": "^1.11.5",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.11",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}